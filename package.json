  "bugs": {
    "url": "https://github.com/NikolasSkrlj/Zavrsni-rad/issues"
  },
  "homepage": "https://github.com/NikolasSkrlj/Zavrsni-rad#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "d3-time": "^1.1.0",
    "date-fns": "^2.15.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.15",
    "multer": "^1.4.2",
    "sharp": "^0.25.4",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "jest": "^26.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4"
  }
